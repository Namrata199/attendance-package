{
    "name": "namratalohani/filament-hr-system",
    "description": "HR System made with laravel, filament and livewire",
    "keywords": [
        "namratalohani",
        "filament-hr-system"
    ],
    "homepage": "https://github.com/namratalohani/filament-hr-system",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Namrata Lohani",
            "email": "namru.mail@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Namratalohani\\FilamentHrSystem\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Namratalohani\\FilamentHrSystem\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Namratalohani\\FilamentHrSystem\\FilamentHrSystemServiceProvider"
            ],
            "aliases": {
                "FilamentHrSystem": "Namratalohani\\FilamentHrSystem\\FilamentHrSystemFacade"
            }
        }
    }
}
